<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:SyncoStronbo" xmlns:Pages="clr-namespace:SyncoStronbo.Pages"
    x:Class="SyncoStronbo.AppShell"
    >

    <FlyoutItem Title="Home" >

        <ShellContent ContentTemplate="{DataTemplate TypeName=Pages:Main}" />


    </FlyoutItem>

    <FlyoutItem Title="New" Route="Create" >
        
        <ShellContent Title="New" ContentTemplate="{DataTemplate TypeName=local:MainPage}" />

    </FlyoutItem>

    <FlyoutItem Title="Join" >

        <ShellContent Title="Join" ContentTemplate="{DataTemplate TypeName=local:MainPage}" />

    </FlyoutItem>



</Shell>
